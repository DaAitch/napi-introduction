  "version": "0.2.0",
  "configurations": [
    {
      "name": "NAPI C",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/env",
      "args": ["node", "index.js"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/napi-c",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }, {
      "name": "NAPI CC",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/env",
      "args": ["node", "index.js"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/napi-cc",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}